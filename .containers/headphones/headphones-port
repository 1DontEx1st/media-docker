  # Headphones
  headphones:
    image: linuxserver/headphones:latest
    container_name: headphones
    volumes:
      - ${BASE_DIR}/headphones/config:/config
      - ${MEDIA_DIR}/Music:/music
      - ${DOWNLOAD_DIR}:/downloads
    environment:
      - PUID=1000
      - PGID=1000
      - TZ=${TIMEZONE}
    networks:
      - internal
      - proxied
    ports:
      - 8182:8181
    restart: always
