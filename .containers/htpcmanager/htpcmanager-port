  # HTPC Manager
  htpcmanager:
    image: linuxserver/htpcmanager:latest
    container_name: htpcmanager
    volumes:
      - ${BASE_DIR}/htpcmanager/config:/config
    environment:
      - PUID=1000
      - PGID=1000
      - TZ=${TIMEZONE}
    networks:
      - internal
      - proxied
    ports:
      - 8085:8085
    restart: always