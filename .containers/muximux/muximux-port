services:
  # Muximux
  muximux:
    image: linuxserver/muximux:latest
    container_name: muximux
    volumes:
      - ${BASE_DIR}/muximux/config:/config
    environment:
      - PUID=1000
      - PGID=1000
      - TZ=${TIMEZONE}
    networks:
      - internal
      - proxied
    ports:
      - 82:80
    restart: always
